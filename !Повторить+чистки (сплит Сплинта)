    >>> "А роза упала на лапу Азора".split()
    ['А', 'роза', 'упала', 'на', 'лапу', 'Азора']

    >>> "А роза упала на лапу Азора".split('а')
    ['А роз', ' уп', 'л', ' н', ' л', 'пу Азор', '']

    >>> "\t  \n  строка с ерундой по краям   \t  \n\n".strip()
    'строка с ерундой по краям'#Для очистки от кожур\tы и семечn\ек, а так же корабельных кры с

    >>> "авбвааббв Три слона на перепутье бббаааввв".strip("абв") #С важным уточнением
    ' Три слона на перепутье '

    >>> "авбвааббв Три слона на баобабе бббаааввв".removeprefix("абв")#от конкретных последовательностей
    'авбвааббв Три слона на баобабе бббаааввв'
    >>> "авбвааббв Три слона на баобабе бббаааввв".removeprefix("авб")
    'вааббв Три слона на баобабе бббаааввв'
    >>> "авбвааббв Три слона на баобабе бббаааввв".removesuffix("ааввв")
    'авбвааббв Три слона на баобабе ббба'#че они там вообще забыли...
